namespace BayLang;


static class ModuleDescription
{
	/**
	 * Returns module name
	 * @return string
	 */
	pure string getModuleName() => "BayLang";
	
	
	/**
	 * Returns module name
	 * @return string
	 */
	pure string getModuleVersion() => "0.12.3";
	
	
	/**
	 * Returns required modules
	 * @return Map<string>
	 */
	pure Map<string> requiredModules() =>
	{
		'Runtime': '>=0.11 <1.0',
	};
	
	
	/**
	 * Returns enities
	 */
	pure Collection<Dict> entities() => null;
}
