namespace BayrellLang.OpCodes;

use BayrellLang.OpCodes.BaseOpCode;


export class OpCompare extends BaseOpCode{
	
	public string op = "op_compare";
	public string cond = null;
	public BaseOpCode value1 = null;
	public BaseOpCode value2 = null;
	
	
	/**
	 * Returns classname of the object
	 * @return string
	 */
	public string getClassName(){
		return "BayrellLang.OpCodes.OpCompare";
	}
	
	
	
	/**
	 * Constructor
	 */
	void constructor(string cond = "", BaseOpCode value1 = null, BaseOpCode value2 = null){
		parent();
		this.cond = cond;
		this.value1 = value1;
		this.value2 = value2;
	}
	
	
	
	/**
	 * Destructor
	 */
	void destructor(){
		parent();
	}
	
	
	
	/**
	 * Returns name of variables to serialization
	 * @return Vector<string>
	 */
	public void getVariablesNames(Vector<string> names){
		parent::getVariablesNames(names);
		names.push("cond");
		names.push("value1");
		names.push("value2");
	}
	
	
	
	/**
	 * Returns instance of the value by variable name
	 * @param string variable_name
	 * @return var
	 */
	public var takeValue(string variable_name, var default_value = null){
		if (variable_name == 'cond') return this.cond;
		if (variable_name == 'value1') return this.value1;
		if (variable_name == 'value2') return this.value2;
		return parent::takeValue(variable_name, default_value);
	}
	
	
	
	/**
	 * Set new value instance by variable name
	 * @param string variable_name
	 * @param var value
	 */
	public void assignValue(string variable_name, var value){
		if (variable_name == 'cond') this.cond = value;
		else if (variable_name == 'value1') this.value1 = value;
		else if (variable_name == 'value2') this.value2 = value;
		else
			parent::assignValue(variable_name, value);
	}
	
	
	
	/**
	 * Assign all data from other object
	 * @param CoreObject obj
	 */
	void assign(CoreObject obj){
		if (obj instanceof OpCompare){
			this.cond = clone obj.cond;
			this.value1 = clone obj.value1;
			this.value2 = clone obj.value2;
		}
		parent::assign(obj);
	}
}