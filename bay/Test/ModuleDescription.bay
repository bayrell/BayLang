namespace BayLang.Test;

use Runtime.Unit.UnitTest;
use BayLang.ModuleDescription as BayLangDescription;


static class ModuleDescription
{
	/**
	 * Returns module name
	 * @return string
	 */
	pure string getModuleName() => "BayLang.Test";
	
	
	/**
	 * Returns module name
	 * @return string
	 */
	pure string getModuleVersion() => BayLangDescription::getModuleVersion();
	
	
	/**
	 * Returns required modules
	 * @return Map<string>
	 */
	pure Map<string> requiredModules() =>
	{
		'BayLang': '>=0.12',
	};
	
	
	/**
	 * Returns enities
	 */
	pure Collection<Dict> entities() => [
		new UnitTest("BayLang.Test.LangBay.Base"),
		new UnitTest("BayLang.Test.LangBay.Expression"),
		new UnitTest("BayLang.Test.LangBay.Html"),
		new UnitTest("BayLang.Test.LangBay.Operator"),
		new UnitTest("BayLang.Test.LangBay.Program"),
	];
}
